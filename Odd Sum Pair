#include <bits/stdc++.h>
using namespace std;

int main() {
    
    int tt;
    cin>>tt;
    while(tt--) {
        
        int a,b,c;
        cin>>a>>b>>c;
        
        if((a + b) % 2 == 1 || (a + c) % 2 == 1 || (b + c) % 2 == 1) {
            cout<<"YES"<<endl;
        }
        else {
           cout<<"NO"<<endl;
        }
    }
}
